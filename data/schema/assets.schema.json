{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "assets",
  "type": "object",
  "properties": {
    "asset_id": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": [
        "bridge",
        "pavement",
        "water"
      ]
    },
    "age_years": {
      "type": "number",
      "minimum": 0
    },
    "traffic_aadt": {
      "type": "number",
      "minimum": 0
    },
    "importance": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    }
  },
  "required": [
    "asset_id",
    "type",
    "age_years",
    "importance"
  ]
}